#!/usr/bin/fish

mpd &
clipmenud &
#backupd &
superhudd &
gpgconf --launch gpg-agent &
xautolock \
	-resetsaver -detectsleep -time 10 -locker "powerctl lock" -killtime 20 -killer "powerctl suspend" \
	-notify 10 -notifier "notify-send -i system-suspend 'Idle Warning' 'Locking screen in 10 seconds...'" \
	-corners "----" -cornersize 5 &
polybar --config=$HOME/.config/polybar/config.ini main &
